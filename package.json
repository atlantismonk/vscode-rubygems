{
  "name": "vscode-rubygems",
  "displayName": "Rubygems",
  "description": "The Rubygems Explorer can browser RubyGems source codes of your project by `Gemfile.lock`",
  "version": "1.0.1",
  "main": "./dist/extension.js",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.2"
  },
  "categories": [
    "Extension Packs"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "pack": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./dist/test/runTest.js",
    "lint": "eslint src/**/*.ts --fix",
    "pretty": "prettier --config .prettierrc.js --loglevel warn --write \"./**/*.{ts,md,json}\""
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "open": "^7.1.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.158",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.51",
    "@types/rimraf": "^2.0.4",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "mocha": "^6.2.3",
    "prettier": "^2.1.2",
    "typescript": "^3.9.7",
    "vsce": "^1.81.1",
    "vscode-test": "^1.4.0"
  },
  "publisher": "xxamxx",
  "author": {
    "name": "am",
    "url": "https://github.com/xxamxx"
  },
  "homepage": "https://github.com/xxamxx/vscode-rubygems",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxamxx/vscode-rubygems.git"
  },
  "bugs": {
    "url": "https://github.com/xxamxx/vscode-rubygems/issues"
  },
  "icon": "resources/image/icon.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "activationEvents": [
    "onView:rubygems.explorer",
    "workspaceContains:**/Gemfile.lock"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": []
    },
    "views": {
      "explorer": [
        {
          "id": "rubygems.explorer",
          "name": "rubygems"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rubygems.command.refresh",
          "when": "view == rubygems.explorer",
          "group": "navigation"
        },
        {
          "command": "rubygems.command.focus",
          "when": "view == rubygems.explorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "rubygems.command.open-rubygems-website",
          "when": "view == rubygems.explorer && viewItem == rubygems.explorer.specitem",
          "group": "inline"
        },
        {
          "command": "rubygems.command.filter-deps",
          "when": "view == rubygems.explorer && viewItem == rubygems.explorer.specitem",
          "group": "inline"
        },
        {
          "command": "rubygems.command.filter-reqs",
          "when": "view == rubygems.explorer && viewItem == rubygems.explorer.specitem",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "rubygems.command.refresh",
        "icon": "$(refresh)",
        "category": "Rubygems",
        "title": "Reload RubyGems"
      },
      {
        "command": "rubygems.command.all-collapsed",
        "icon": "$(collapse-all)",
        "category": "Rubygems",
        "title": "Collapsed"
      },
      {
        "command": "rubygems.command.focus",
        "icon": "$(record)",
        "category": "Rubygems",
        "title": "Focus"
      },
      {
        "command": "rubygems.command.filter-deps",
        "icon": "$(filter)",
        "category": "Rubygems",
        "title": "Filter Dependentcies"
      },
      {
        "command": "rubygems.command.filter-reqs",
        "icon": "$(list-filter)",
        "category": "Rubygems",
        "title": "Filter Requirements"
      },
      {
        "command": "rubygems.command.search",
        "icon": "$(search)",
        "category": "Rubygems",
        "title": "Search"
      },
      {
        "command": "rubygems.command.open-rubygems-website",
        "icon": "$(link-external)",
        "category": "Rubygems",
        "title": "Open RubyGems Website"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Rubygems",
      "properties": {
        "rubygems.other.website": {
					"type": "string",
					"default": "https://rubygems.org/search?query=${name}",
					"markdownDescription": "The Ruby communityâ€™s gem hosting service.",
					"scope": "resource"
				}
      }
    }
  }
}
